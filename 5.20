#include<iostream>
using namespace std;
main(){
	int s1,s2,hyp;
	int sol=0;
	
	cout<<" first side\t second side\t hypotenus\n";
	for(s1=1;s1<=500;s1++){
		for(s2=1;s2<=500;s2++){
			for(hyp=0;hyp<=500;hyp++){
				
		if((s1*s1)+(s2*s2)==(hyp*hyp)){
		cout<<s1<<"\t"<<s2<<"\t"<<hyp<<"\n";
		sol++;
		}
		}
	}

	}
	cout<<"there are "<<sol<<" possible solutions";
}
	 
