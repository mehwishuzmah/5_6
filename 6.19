#include<iostream>
#include<cmath>
using namespace std;
double hypotenus(double s1,double s2){
	
    int  h=sqrt((s1*s1)+(s2*s2));
    return h;
}
main(){
	float sid1,sid2;
	sid1=3,sid2=4;
	cout<<"triangle side1 side2 hypotenus\n";
	cout<<"1\t3.0\t4.0\t"<<hypotenus(sid1,sid2)<<"\n";

sid1=5,sid2=12;
cout<<"2\t5.0\t12.0\t"<<hypotenus(sid1,sid2)<<"\n";
sid1=8,sid2=15;
cout<<"3\t8.0\t15.0\t"<<hypotenus(sid1,sid2)<<"\n";
}
