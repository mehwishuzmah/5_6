(a)
#include<iostream>
using namespace std;
int main(){
	for(int i=1;i<=10;i++){
		for(int j=1;j<=i;j++){
			cout<<"*";
		}
		cout<<endl;
	}
return 0;
}

(b)
#include<iostream>
using namespace std;
int main(){
	for(int i=10;i>=1;i--){
		for(int j=1;j<=i;j++){
			cout<<"*";
			
		}
	
		cout<<endl;
	}
return 0;
}


(c)

#include<iostream>
using namespace std;
main(){
	for(int i=10;i>0;i--)
	{
		for(int j=10;j>0;j--){
			if(j<=i)
			cout<<"*";
			else
			cout<<" ";
					}
					cout<<"\n";
					
	}
}

(d)

#include<iostream>
using namespace std;
main(){
	for(int i=0;i<=10;i++){
		for(int j=10;j>=0;j--){
			if(j<=i)
			cout<<"*";
			else
			cout<<" ";
		}
		cout<<"\n";
	}
}
