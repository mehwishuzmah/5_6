 #include <iostream>
using namespace std;
 double smallest( double min, double n2, double n3 ){
 	 if ( n2 < min )
	 min = n2;
    if(n3 < min ) 
	min=n3;
	return min;

 	
 }
 int main() {

    double d1, d2, d3;

    cout << "Three double numbers: ";
    cin >> d1 >> d2 >> d3;
    cout << "Smallest is " << smallest( d1, d2, d3 ) << endl;

    return 0;

 }



 
